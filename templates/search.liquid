{% paginate search.results by 4%}


{% if search.performed %} 
<h1> Here your search result</h1>

 {% for result in search.results %}
   {% if result.object_type == "page" %}
     <a href = "{{result.url }}"> 
      {{result.title}}
      </a>

    {% elsif result.object_type =="article" %}
      <a href = "{{result.url }}"> 
      {{result.title}}
      </a>
    {% else %}
      <a href = "{{result.url }}"> 
      {{result.title}}
      </a>
    {% endif %}
  {% endfor%}

{% else %}
<h1> Try to search something</h1>
{% endif %}

{{ paginate |default_pagination}}
{% endpaginate %}